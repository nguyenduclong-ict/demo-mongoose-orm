  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "tsnd --rs --respawn -r dotenv/config -r ./scripts/tsconfig-paths-bootstrap.js --project server/tsconfig.json --files server/index.ts dotenv_config_path=.env.dev",
    "dev:admin": "nuxt -c ./admin/nuxt.config.js --dotenv .env.dev",
    "dev": "node ./scripts/dev.js",
    "build:server": "tsc --p server",
    "build:admin": "nuxt generate -c ./admin/nuxt.config.js --dotenv .env",
    "build": "yarn build:server && yarn build:admin",
    "start": "node -r dotenv/config -r ./scripts/tsconfig-paths-bootstrap.js scripts/start.js"
  },
